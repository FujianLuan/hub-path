{
    "manifest_version": 2,
    "name": "Hub Path",
    "version": "0.1",

    "description": "Change the blocked github to the mirror path",
    "icons": {
        "48": "icon/icon-48.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["path-replacer.js"]
        }
    ]
}